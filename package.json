{
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "name": "link-storage-monorepo",
  "version": "1.0.0",
  "description": "Link Storage Monorepo with pnpm workspaces",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "install:all": "pnpm install",
    "api:dev": "pnpm --filter @link-storage/api start:dev",
    "api:build": "pnpm --filter @link-storage/api build",
    "api:start": "pnpm --filter @link-storage/api start:prod",
    "api:prisma:generate": "pnpm --filter @link-storage/api prisma:generate",
    "api:prisma:migrate": "pnpm --filter @link-storage/api prisma:migrate",
    "api:prisma:push": "pnpm --filter @link-storage/api prisma:push",
    "api:prisma:studio": "pnpm --filter @link-storage/api prisma:studio"
  },
  "keywords": [
    "monorepo",
    "pnpm",
    "workspaces",
    "link-storage"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "prettier": "^3.0.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
